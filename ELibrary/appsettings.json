{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=.;Database=ELibraryDb;Integrated Security=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "LocalizationOptions": {
    "ResourcesPath": "i18N",
    "CacheDuration": "00:00:15",
    "DefaultCulture": "en-US",
    "DefaultUICulture": "en-US",
    "SupportedCultureInfos": [ "en-US", "ar-AE" ],
    "IsAbsolutePath": true,
    "FileEncodingName": "utf-8",
    "UseBaseName": false
  }
}
